function mouseOver(e){
var e=(e||event);
var o=e["target"]||e["srcElement"];
var audio_file = new Audio(o.id+'.ogg');
a=document.getElements
audio_file.load();
audio_file.volume=1
audio_file.play();
var t = setInterval(function(){
    if(audio_file.volume<=1){
        audio_file.volume+=0.001;
    }
	else{
        clearInterval(t);
    }
    },2);
}
function mouseOut(){
var t2 = setInterval(function(){
    if(audio_file.volume>=0){
        audio_file.volume-=0.001;
    }
	else{
        clearInterval(t2);
		audio_file.load();
    }
    },2);
}